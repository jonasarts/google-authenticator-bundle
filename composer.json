{
    "name": "jonasarts/google-authenticator-bundle",
    "description": "A Google Authenticator Bundle for Symfony",
    "keywords": ["Google", "Authenticator", "HOTP", "TOTP"],
    "homepage": "https://github.com/jonasarts/google-authenticator-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hauser",
            "email": "symfony@jonasarts.com",
            "homepage": "http://www.jonasarts.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5"
    },
    "autoload": {
        "psr-4": {
            "jonasarts\\GoogleAuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jonasarts\\GoogleAuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "target-dir": "jonasarts/GoogleAuthenticatorBundle"
}
